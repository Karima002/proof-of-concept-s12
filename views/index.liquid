            <div class="content">
                <p class="tekst-icons"> {{ huis.description }}</p>
                <label for="check"> 
                    <img src="/assets/line.png" alt="icoon van een streep">
                    <p>Verberg omschrijving</p>
                </label>
            </div>

            <label for="check">
                <img src="/assets/plus.png" alt="icoon van een plus">
                <p> Lees de volledige omschrijving </p>
            </label>
        </article>
                    
