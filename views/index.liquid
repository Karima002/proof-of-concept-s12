        <div class="desktop">
            <section class="adres">
                <h1 class="straat"> {{ huis.street }} 11</h1>
                <p class="tekst-postcode"> {{ huis.postal_code }} {{ huis.city }}</p>
                <img class="image-kaart-adres" src="/assets/map.png" alt="image van plaats in Amsterdam">
            </section>

            <!-- MARK: Informatie huis-->
            <section class="info-oppervlakte"> 

                {% render 'partials/icons/oppervlakte.liquid' %}
                <p class="tekst-icons"> {{ huis.m2_garden }} m²</p>

                {% render 'partials/icons/oppervlakte2.liquid' %}
                <p class="tekst-icons"> {{ huis.m2 }} m²</p>


                {% render 'partials/icons/room.liquid' %}
                <p class="tekst-icons"> {{ huis.rooms }}</p>
                
               {% render 'partials/icons/electric.liquid' %}
